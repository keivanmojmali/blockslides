{
  "name": "@blockslides/markdown",
  "description": "markdown parser and serializer for blockslides",
  "version": "0.1.0",
  "keywords": [
    "blockslides",
    "blockslides utility"
  ],
  "license": "MIT",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/index.d.ts",
        "require": "./dist/index.d.cts"
      },
      "import": {
        "browser": "./dist/index.js",
        "default": "./dist/index.js"
      },
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "src",
    "dist"
  ],
  "dependencies": {
    "marked": "^16.1.2"
  },
  "devDependencies": {
    "@blockslides/core": "workspace:^",
    "@blockslides/pm": "workspace:^"
  },
  "peerDependencies": {
    "@blockslides/core": "workspace:^",
    "@blockslides/pm": "workspace:^"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/keivanmojmali/blockslides",
    "directory": "packages/markdown"
  },
  "scripts": {
    "build": "tsup",
    "lint": "prettier ./src/ --check && eslint --cache --quiet --no-error-on-unmatched-pattern ./src/"
  },
  "author": "keivanmojmali"
}
