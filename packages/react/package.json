{
  "name": "@autoartifacts/react",
  "version": "0.0.1",
  "description": "React components for AutoArtifacts slide editor",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./src/index.ts",
      "types": "./src/index.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --external react --external react-dom --external @autoartifacts/styles",
    "dev": "tsup src/index.ts --format cjs,esm --watch --external react --external react-dom --external @autoartifacts/styles"
  },
  "dependencies": {
    "@autoartifacts/core": "workspace:*",
    "prosemirror-commands": "^1.7.1",
    "prosemirror-history": "^1.4.1",
    "prosemirror-keymap": "^1.2.3",
    "prosemirror-model": "^1.25.3",
    "prosemirror-state": "^1.4.3",
    "prosemirror-view": "^1.41.2"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "react",
    "autoartifacts",
    "editor",
    "slides"
  ],
  "author": "",
  "license": "ISC"
}

