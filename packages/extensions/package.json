{
  "name": "@blockslides/extensions",
  "description": "various extensions for blockslides",
  "version": "0.1.1",
  "homepage": "https://github.com/keivanmojmali/blockslides",
  "keywords": [
    "blockslides",
    "blockslides extension"
  ],
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/index.d.ts",
        "require": "./dist/index.d.cts"
