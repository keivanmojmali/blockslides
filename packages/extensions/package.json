      },
      "import": "./dist/trailing-node/index.js",
      "require": "./dist/trailing-node/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "src",
    "dist"
  ],
  "dependencies": {
    "@blockslides/core": "workspace:^",
    "@blockslides/pm": "workspace:^"
  },
  "devDependencies": {
    "@blockslides/core": "workspace:^",
    "@blockslides/pm": "workspace:^"
  },
  "peerDependencies": {
    "@blockslides/core": "workspace:^",
    "@blockslides/pm": "workspace:^"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/keivanmojmali/blockslides",
    "directory": "packages/extensions"
  },
  "scripts": {
    "build": "tsup",
    "lint": "prettier ./src/ --check && eslint --cache --quiet --no-error-on-unmatched-pattern ./src/"
  },
  "author": "keivanmojmali"
