{
  "name": "@autoartifacts/core",
  "version": "0.0.1",
  "description": "Core ProseMirror-based slide editor with essential plugins",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./jsx-runtime": {
      "import": "./jsx-runtime/index.js",
      "require": "./jsx-runtime/index.cjs",
      "types": "./jsx-runtime/index.d.ts"
    },
    "./jsx-dev-runtime": {
      "import": "./jsx-dev-runtime/index.js",
      "require": "./jsx-dev-runtime/index.cjs",
      "types": "./jsx-dev-runtime/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "prosemirror-commands": "^1.7.1",
    "prosemirror-history": "^1.4.1",
    "prosemirror-inputrules": "^1.4.0",
    "prosemirror-keymap": "^1.2.3",
    "prosemirror-model": "^1.25.3",
    "prosemirror-schema-basic": "^1.2.4",
    "prosemirror-state": "^1.4.3",
    "prosemirror-view": "^1.41.2"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/jsdom": "^27.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jsdom": "^27.0.0",
    "prosemirror-test-builder": "^1.1.1",
    "ts-jest": "^29.4.5",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "prosemirror",
    "editor",
    "slides",
    "presentation"
  ],
  "author": "",
  "license": "ISC"
}
