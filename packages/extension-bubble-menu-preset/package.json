  "peerDependencies": {
    "@blockslides/core": "workspace:^",
    "@blockslides/extension-bubble-menu": "workspace:^",
    "@blockslides/extension-highlight": "workspace:^",
    "@blockslides/extension-link": "workspace:^",
    "@blockslides/extension-text-align": "workspace:^",
    "@blockslides/extension-text-style": "workspace:^",
    "@blockslides/extensions": "workspace:^",
    "@blockslides/pm": "workspace:^"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/keivanmojmali/blockslides",
    "directory": "packages/extension-bubble-menu-preset"
