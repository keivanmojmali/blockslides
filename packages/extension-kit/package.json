{
  "name": "@autoartifacts/extension-kit",
  "description": "Complete extension kit for autoartifacts - includes all available extensions with configurable options",
  "version": "0.0.1",
  "homepage": "https://github.com/keivanmojmali/autoartifacts",
  "keywords": [
    "autoartifacts",
    "editor",
    "slides",
    "extension-kit",
    "extensions"
  ],
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/index.d.ts",
        "require": "./dist/index.d.cts"
      },
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "dependencies": {
    "@autoartifacts/core": "workspace:*",
    "@autoartifacts/extension-blockquote": "workspace:*",
    "@autoartifacts/extension-bold": "workspace:*",
    "@autoartifacts/extension-bubble-menu": "workspace:*",
    "@autoartifacts/extension-bullet-list": "workspace:*",
    "@autoartifacts/extension-code": "workspace:*",
    "@autoartifacts/extension-code-block": "workspace:*",
    "@autoartifacts/extension-code-block-lowlight": "workspace:*",
    "@autoartifacts/extension-color": "workspace:*",
    "@autoartifacts/extension-details": "workspace:*",
    "@autoartifacts/extension-document": "workspace:*",
    "@autoartifacts/extension-emoji": "workspace:*",
    "@autoartifacts/extension-file-handler": "workspace:*",
    "@autoartifacts/extension-floating-menu": "workspace:*",
    "@autoartifacts/extension-font-family": "workspace:*",
    "@autoartifacts/extension-hard-break": "workspace:*",
    "@autoartifacts/extension-heading": "workspace:*",
    "@autoartifacts/extension-highlight": "workspace:*",
    "@autoartifacts/extension-horizontal-rule": "workspace:*",
    "@autoartifacts/extension-image": "workspace:*",
    "@autoartifacts/extension-invisible-characters": "workspace:*",
    "@autoartifacts/extension-italic": "workspace:*",
    "@autoartifacts/extension-link": "workspace:*",
    "@autoartifacts/extension-list": "workspace:*",
    "@autoartifacts/extension-mathematics": "workspace:*",
    "@autoartifacts/extension-node-range": "workspace:*",
    "@autoartifacts/extension-ordered-list": "workspace:*",
    "@autoartifacts/extension-paragraph": "workspace:*",
    "@autoartifacts/extension-placeholder": "workspace:*",
    "@autoartifacts/extension-slide": "workspace:*",
    "@autoartifacts/extension-add-slide-button": "workspace:*",
    "@autoartifacts/extension-strike": "workspace:*",
    "@autoartifacts/extension-subscript": "workspace:*",
    "@autoartifacts/extension-superscript": "workspace:*",
    "@autoartifacts/extension-table": "workspace:*",
    "@autoartifacts/extension-table-of-contents": "workspace:*",
    "@autoartifacts/extension-text": "workspace:*",
    "@autoartifacts/extension-text-align": "workspace:*",
    "@autoartifacts/extension-text-style": "workspace:*",
    "@autoartifacts/extension-typography": "workspace:*",
    "@autoartifacts/extension-underline": "workspace:*",
    "@autoartifacts/extension-unique-id": "workspace:*",
    "@autoartifacts/extension-youtube": "workspace:*",
    "@autoartifacts/extensions": "workspace:*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/keivanmojmali/autoartifacts",
    "directory": "packages/extension-kit"
  },
  "scripts": {
    "build": "tsup"
  }
}
