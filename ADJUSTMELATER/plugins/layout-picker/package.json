{
  "name": "@autoartifacts/plugin-layout-picker",
  "version": "0.0.1",
  "description": "Layout picker plugin for AutoArtifacts - choose from predefined slide layouts",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./src/index.ts",
      "types": "./src/index.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm",
    "dev": "tsup src/index.ts --format cjs,esm --watch"
  },
  "dependencies": {
    "@autoartifacts/core": "workspace:*",
    "prosemirror-state": "^1.4.3",
    "prosemirror-view": "^1.41.2",
    "prosemirror-model": "^1.25.3"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "autoartifacts",
    "plugin",
    "layout",
    "templates"
  ],
  "author": "",
  "license": "ISC"
}

